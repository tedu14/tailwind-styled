{
  "name": "tailwindcss-styled",
  "private": false,
  "version": "1.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "A library for building styled components with Tailwind CSS",
  "author": "Thalison eduardo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedu14/tailwind-styled.git"
  },
  "homepage": "https://github.com/tedu14/tailwind-styled#readme",
  "bugs": {
    "url": "https://github.com/tedu14/tailwind-styled/issues"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "styled",
    "styled-components",
    "components",
    "react",
    "typescript",
    "css",
    "ui",
    "design-system"
  ],
  "main": "./dist/tailwindcss-styled.cjs.js",
  "module": "./dist/tailwindcss-styled.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/tailwindcss-styled.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/tailwindcss-styled.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build",
    "publish:check": "npm pack --dry-run",
    "publish:local": "npm pack",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "react": "^19.2.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
}